{
  "compilerOptions": {
    "jsx": "react",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "assets/*": ["src/assets/*"],
    },
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
  }
}